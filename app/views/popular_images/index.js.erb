<% @images.each do |image| %>
  $('#news-feed-images')
    .append('<%= j render "images/image", image: image %>');
<% end %>
<% if @image_support.last %>
  $('#load-more-news-feed').hide();
  $('.news-feed-middle-content').removeClass('with-padding');
<% else %>
  $('#load-more-news-feed')
    .html('<%= link_to t("image_action.load_more"), popular_images_path(
    image_offset: @image_support.last.id), remote: true %>');
<% end %>
